{
    "configurations": [
        {
            "name": "(gdb) fast_dla.bin",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/fast_dla.dbg",
            "args": ["-seed", "1", "-ts", "1000", "-tmax", "1000", "-num_bind", "100", "-mode", "n"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}